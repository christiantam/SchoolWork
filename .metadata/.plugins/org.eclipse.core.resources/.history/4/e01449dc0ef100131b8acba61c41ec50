import java.math.BigInteger;


public class p020 {
	public static void main (String [] args){
		BigInteger num = BigInteger.valueOf(factorial(30));
		System.out.println(num.toString());
	}
	
	public static long factorial (int num){
		long ans = 1;
		for(int i = num; i > 1; i--){
			ans *= i;
		}
		return ans;
	}
}
